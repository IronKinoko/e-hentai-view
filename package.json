{
  "name": "e-hentai-view",
  "productName": "EhentaiView",
  "version": "1.1.0",
  "main": "main/index.js",
  "license": "MIT",
  "author": {
    "name": "kinoko",
    "email": "kinoko.main@gmail.com",
    "url": "https://github.com/IronKinoko"
  },
  "repository": {
    "url": "https://github.com/IronKinoko/e-hentai-view.git",
    "type": "git"
  },
  "scripts": {
    "clean": "rimraf dist main renderer/out renderer/.next",
    "start": "electron .",
    "build-renderer": "next build renderer && next export renderer",
    "build-electron": "tsc -p electron-src",
    "build": "npm run build-renderer && npm run build-electron",
    "pack-app": "npm build && electron-builder --dir",
    "dist": "npm run clean && npm run build && npm run electron-builder",
    "analyze": "cross-env ANALYZE=true next build renderer",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -w -s -r 0",
    "lint": "eslint renderer --fix --ext .ts,.tsx"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "build": {
    "asar": true,
    "files": [
      "main",
      "renderer/out"
    ],
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "runAfterFinish": true
    }
  },
  "devDependencies": {
    "@types/node": "^12.12.21",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@typescript-eslint/eslint-plugin": "^3.0.1",
    "@typescript-eslint/parser": "^3.0.1",
    "electron": "^7.1.7",
    "electron-builder": "^21.2.0",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.2",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.2.5",
    "next": "9.4.0",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.0",
    "typescript": "latest"
  },
  "dependencies": {
    "@material-ui/core": "^4.9.13",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.52",
    "@next/bundle-analyzer": "^9.4.0",
    "@types/file-saver": "^2.0.1",
    "@types/lodash": "^4.14.150",
    "@types/qs": "^6.9.2",
    "@umijs/hooks": "^1.9.2",
    "axios": "^0.19.2",
    "babel-plugin-import": "^1.13.0",
    "clsx": "^1.1.0",
    "conventional-changelog-cli": "^2.0.34",
    "cross-env": "^7.0.2",
    "electron-is-dev": "^1.1.0",
    "electron-next": "^3.1.5",
    "file-saver": "^2.0.2",
    "filesize": "^6.1.0",
    "jszip": "^3.4.0",
    "lodash": "^4.17.15",
    "mobx": "^5.15.4",
    "mobx-react-lite": "^2.0.6",
    "moment": "^2.25.3",
    "nprogress": "^0.2.0",
    "qs": "^6.9.4",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-hook-form": "^5.6.3"
  }
}
